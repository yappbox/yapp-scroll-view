{
  "name": "yapp-scroll-view",
  "version": "11.1.0",
  "description": "Ember Scrollling components using a Javascript scroller",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yappbox/yapp-scroll-view.git"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "release": "release-it",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@ember/render-modifiers": "^3.0.0",
    "@ember/string": "^4.0.1",
    "@ember/test-waiters": "^4.1.1",
    "@embroider/macros": "^1.19.1",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-stew": "^3.0.0",
    "ember-auto-import": "^2.11.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-concurrency": "^5.1.0",
    "ember-on-resize-modifier": "^2.0.2",
    "ember-ref-bucket": "^5.0.8",
    "ember-set-helper": "^3.1.0",
    "ember-truth-helpers": "^4.0.3",
    "eventemitter3": "^5.0.1",
    "hammerjs": "^2.0.8",
    "macro-decorators": "^0.1.2",
    "tracked-built-ins": "^4.0.0",
    "webpack": "^5.95.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.3",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/eslint-parser": "^7.26.3",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^3.2.0",
    "@embroider/test-setup": "^4.0.0",
    "@glimmer/component": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~5.4.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-deploy": "^2.0.0",
    "ember-cli-deploy-build": "^3.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deprecation-workflow": "^3.4.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-collection": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^7.0.0",
    "ember-resolver": "^13.1.1",
    "ember-simulant-test-helpers": "^0.3.2",
    "ember-source": "~4.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^7.9.3",
    "ember-try": "^4.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ember": "^12.7.4",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-qunit": "^8.2.5",
    "layout-bin-packer": "~2.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "qunit": "^2.20.0",
    "qunit-dom": "^2.0.0",
    "release-it": "^19.0.5",
    "@release-it-plugins/lerna-changelog": "^8.0.1",
    "sass": "^1.93.2"
  },
  "engines": {
    "node": ">= 20",
    "pnpm": ">= 9"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://yappbox.github.io/yapp-scroll-view"
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    },
    "npm": {
      "publish": false
    }
  },
  "volta": {
    "node": "20.19.5",
    "pnpm": "9.15.9"
  },
  "packageManager": "pnpm@9.15.9"
}
