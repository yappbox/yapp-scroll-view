{
  "name": "yapp-scroll-view",
  "version": "7.0.3",
  "description": "Ember Scrollling components using a Javascript scroller",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yappbox/yapp-scroll-view.git"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:js": "eslint .",
    "release": "release-it",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@ember/render-modifiers": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "broccoli-stew": "^2.1.0",
    "ember-auto-import": "^1.6.0",
    "ember-cached-decorator-polyfill": "^0.1.0",
    "ember-cli-babel": "7.22.1",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-composable-helpers": "^4.3.0",
    "ember-concurrency-decorators": "^2.0.0",
    "ember-concurrency": "^1.2.1",
    "ember-decorators": "^6.1.1",
    "ember-named-blocks-polyfill": "^0.2.3",
    "ember-on-resize-modifier": "^0.3.0",
    "ember-ref-modifier": "^1.0.1",
    "ember-render-helpers": "^0.1.1",
    "ember-resize-observer-polyfill": "^0.0.1",
    "ember-set-helper": "^1.1.0",
    "ember-truth-helpers": "^3.0.0",
    "hammerjs": "^2.0.8",
    "macro-decorators": "^0.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.22.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.3",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-collection": "1.0.0",
    "ember-decorators-polyfill": "^1.1.5",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-simulant-test-helpers": "^0.3.1",
    "ember-source": "~3.22.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.14.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.11.0",
    "eslint-plugin-ember": "^9.3.0",
    "eslint-plugin-node": "^11.1.0",
    "layout-bin-packer": "^1.2.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.5.0",
    "release-it": "^13.6.2",
    "release-it-lerna-changelog": "^2.3.0",
    "sass": "^1.26.8"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://yappbox.github.io/yapp-scroll-view"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    },
    "npm": {
      "publish": false
    }
  }
}
